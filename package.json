{
  "name": "friender",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.3",
    "env-cmd": "^10.1.0",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "redux-thunk": "^2.4.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "cypress:runner": "cypress open --env ENV='dev'",
    "start": "env-cmd -f .env.dev react-scripts start",
    "build": "env-cmd -f .env.dev react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:dev": "npm i && GENERATE_SOURCEMAP=false npm run build && aws s3 sync build s3://dev.friender.io",
    "e2e": "cypress run --env ENV=dev",
    "cy:open": "cypress open --env ENV=dev",
    "cy:run":"cypress run --env ENV=dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.4",
    "@faker-js/faker": "^7.6.0",
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^10.8.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-fill-command": "^1.0.2",
    "esbuild": "^0.14.54",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "sass": "^1.55.0"
  }
}
