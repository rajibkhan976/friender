{
  "name": "friender",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "ag-grid-community": "^28.2.1",
    "ag-grid-react": "^28.2.1",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "dexie": "^3.2.3",
    "dexie-react-hooks": "^1.1.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "history": "^5.3.0",
    "http": "^0.0.1-security",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "randexp": "^0.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lottie-player": "^1.5.0",
    "react-moment": "^1.1.3",
    "react-paginate": "^8.1.4",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.11.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "redux-thunk": "^2.4.1",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "cypress:runner": "cypress open --env ENV='dev'",
    "start": "env-cmd -f .env react-scripts start",
    "build": "env-cmd -f .env react-scripts build",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:beta": "env-cmd -f .env.beta react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:dev": "npm i && GENERATE_SOURCEMAP=false npm run build:dev && aws s3 sync build s3://dev.friender.io",
    "deploy:beta": "npm i && GENERATE_SOURCEMAP=false npm run build:beta && aws s3 sync build s3://beta.friender.io",
    "cy:open:dev": "cypress open --env ENV=dev",
    "cy:open:local": "cypress open --env ENV=local --config defaultCommandTimeout=20000,requestTimeout=7000",
    "cy:run:local-auth": "cypress run --env ENV=local --spec  'cypress/e2e/features/authentication' --config defaultCommandTimeout=15000,requestTimeout=7000",
    "cy:run:local-onboarding": "cypress run --env ENV=local --spec'cypress/e2e/features/onboarding' --config defaultCommandTimeout=15000,requestTimeout=7000 --browser chrome",
    "cy:run:local-profile": "cypress run --env ENV=local --spec 'cypress/e2e/features/profiles' --browser chrome --headed",
    "cy:run:local-settings": "cypress run --env ENV=local --spec 'cypress/e2e/features/settings' --browser chrome --headed",
    "cy:run:local-friends": "cypress run --env ENV=local --spec 'cypress/e2e/features/friends' --browser chrome --headed",
    "delete:report": "rm -rf mochawesome-report/",
    "merge:reports": "mochawesome-merge mochawesome-report/*.json -o cypress-combined-report.json",
    "create:html:report": "npm run merge:reports && marge --reportDir TestReport cypress-combined-report.json",
    "cy:run:local": "npm run delete:report && cypress run --env ENV=local --config defaultCommandTimeout=15000,requestTimeout=7000 ",
    "cy:run:dev": "cypress run --env ENV=dev --config defaultCommandTimeout=15000,requestTimeout=7000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "globals": {
      "chrome": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.2.0",
    "@faker-js/faker": "^7.6.0",
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^10.11.0",
    "cypress-ag-grid": "^2.0.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-fill-command": "^1.0.2",
    "esbuild": "^0.14.54",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "parallelshell": "^3.0.2",
    "sass": "^1.55.0"
  }
}
